					},
					{
						"name": "flood_impacted",
						"type": "String"
					},
					{
						"name": "flood_proof",
						"type": "String"
					},
					{
						"name": "electric_supply",
						"type": "String"
					},
					{
						"name": "dist_from_hub",
						"type": "String"
					},
					{
						"name": "workers_num",
