					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "fmcg_part2.csv",
						"fileSystem": "inputfile"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Ware_house_ID",
